   v-model=\"form.ip\"\n            type=\"text\"\n            placeholder=\"i.e: 127.0.0.1\"\n            required\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group\n          id=\"input-group-2\"\n          label=\"Label\"\n          label-for=\"input-label\"\n          label-align=\"left\"\n          ><b-form-input\n            id=\"input-label\"\n            v-model=\"form.label\"\n            placeholder=\"Put a Label\"\n            type=\"text\"\n            required\n          ></b-form-input>\n        </b-form-group>\n      </b-form>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      form: {\n        ip: \"\",\n        label: \"\",\n      },\n      posts: [],\n      //fields: [\"ip\", \"label\", \"actions\"],\n      filter: \"\",\n      currentPage: 1,\n      perPage: 4,\n    };\n  },\n  computed: {\n    rows() {\n      return this.posts.length;\n    },\n    fields() {\n        return [\n            {\n                key: 'index',\n                label: 'Index'\n            },\n            {\n                key: 'ip',\n                label: 'IP'\n            },\n            {\n                key: 'label',\n                label: 'Label'\n            },\n            {\n                key: 'actions',\n                label: 'Actions'\n            }\n        ];\n    }\n  },\n  methods: {\n    getNetworks: function () {\n      axios({\n        url: \"http://sandboxapi.my.id/api/networks\",\n        method: \"GET\",\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }).then((resp) => {\n        this.posts = resp.data.data;\n      });\n    },\n    resetModal: function() {\n        this.form.ip = '';\n        this.form.label = '';\n    },\n    handleOk(bvModalEvt) {\n        bvModalEvt.preventDefault();\n        this.handleSubmit();\n    },\n    handleSubmit: function () {\n      axios({\n        url: \"http://sandboxapi.my.id/api/networks\",\n        data: this.form,\n        method: \"POST\",\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }).then(() => {\n        this.getNetworks();\n        this.$nextTick(() => {\n          this.$bvModal.hide('modal-add')\n        })\n      });\n    },\n  },\n  mounted() {\n    this.getNetworks();\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}