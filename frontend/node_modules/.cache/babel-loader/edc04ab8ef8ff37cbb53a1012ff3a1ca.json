\",\n          formatter: (value, key, item) => {\n              var date = new Date(item.created_at).toDateString();\n              var time = new Date(item.created_at).toLocaleTimeString();\n              //var dateFormatted = date.getUTCMonth() + ' ' + date.getDay() + ', ' + date.getFullYear();\n              return user + ' ' + date + ' ' + time;\n          }\n        },\n        {\n          key: \"updated_by\",\n          label: \"Updated\",\n        },\n      ];\n    },\n  },\n  methods: {\n    getUser: function (id) {\n      axios({\n        url: \"http://sandboxapi.my.id/api/users/\" + id,\n        method: \"GET\",\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }).then((resp) => {\n        return resp.data.name;\n      });\n    },\n    getLogs: function () {\n      axios({\n        url: \"http://sandboxapi.my.id/api/logs\",\n        method: \"GET\",\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }).then((resp) => {\n        this.items = resp.data.data;\n      });\n    },\n  },\n  mounted() {\n    this.getLogs();\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}