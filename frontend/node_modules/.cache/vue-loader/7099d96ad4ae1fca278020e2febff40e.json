\n\n    <b-row class=\"pb-3\">\n      <b-col sm=\"2\" offset=\"1\">\n        <b-pagination\n          v-model=\"currentPage\"\n          :total-rows=\"rows\"\n          :per-page=\"perPage\"\n        ></b-pagination>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n\n<style scoped>\n    .spacer-1 {\n        height: 15px;\n    }\n    .spacer-2 {\n        height: 30px;\n    }\n    .spacer-3 {\n        height: 45px;\n    }\n</style>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      posts: [],\n      fields: [\"id\", \"ip\", \"label\", \"actions\"],\n      filter: \"\",\n      currentPage: 1,\n      perPage: 4,\n    };\n  },\n  computed: {\n    rows() {\n      return this.posts.length;\n    },\n  },\n  methods: {\n    getnetworks: function () {\n      axios({\n        url: \"http://sandboxapi.my.id/api/networks\",\n        method: \"GET\",\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }).then((resp) => {\n        this.posts = resp.data.data;\n      });\n    },\n  },\n  mounted() {\n    this.getnetworks();\n  },\n};\n</script>\n"]}]}